    "@types/aos": "^3.0.7",
    "@types/leaflet": "^1.9.20",
    "aos": "^2.3.4",
    "framer-motion": "^12.23.12",
    "leaflet": "^1.9.4",
    "lucide-react": "^0.541.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^2.0.5",
    "react-leaflet": "^5.0.0",
    "react-router-dom": "^7.8.2",
    "swiper": "^11.2.10"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
