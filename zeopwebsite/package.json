{
  "name": "zeopwebsite",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "npm run generate-sitemap && tsc -b && vite build",
    "build:no-sitemap": "tsc -b && vite build",
